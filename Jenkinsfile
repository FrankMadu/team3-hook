pipeline {
    agent any
    stages{
        stage('1-make a left'){
            step{
                sh 'echo "walk..."'
            }
        }
        stage('2-make a right'){
            step{
                sh 'echo "walk..."'
            }
        }
        stage('3-make another left'){
            step{
                sh 'echo "walk..."'
            }
        }
        stage('4-cross the street'){
            step{
                sh 'echo "walk..."'
            }
        }
    }
}
